<p id="notice"><%= notice %></p>

<h1>Teams</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Player1</th>
      <th>Player</th>
      <th>Player2</th>
      <th>Player</th>
      <th>Player3</th>
      <th>Player</th>
      <th>Player4</th>
      <th>Player</th>
      <th>Player5</th>
      <th>Player</th>
      <th>Team name</th>
      <th>String</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team.user_id %></td>
        <td><%= team.player1 %></td>
        <td><%= team.player_id %></td>
        <td><%= team.player2 %></td>
        <td><%= team.player_id %></td>
        <td><%= team.player3 %></td>
        <td><%= team.player_id %></td>
        <td><%= team.player4 %></td>
        <td><%= team.player_id %></td>
        <td><%= team.player5 %></td>
        <td><%= team.player_id %></td>
        <td><%= team.team_name %></td>
        <td><%= team.string %></td>
        <td><%= link_to 'Show', team %></td>
        <td><%= link_to 'Edit', edit_team_path(team) %></td>
        <td><%= link_to 'Destroy', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Team', new_team_path %>
